    "version": "0.2.0",
    "configurations": [        
        {
            "name": "Launch Lure",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/lure.go",
            "env": {
                "DRY_RUN": 1,
                "BITBUCKET_CLIENT_ID": "yourid",
                "BITBUCKET_CLIENT_SECRET": "yoursecret",
                "LURE_AUTO_OPEN_AUTH_PAGE": "1"
